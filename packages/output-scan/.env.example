WS_ENDPOINT=wss://kusama-rpc.polkadot.io

MONGO_URL=mongodb://127.0.0.1:27017
MONGO_DB_NAME=dotreasury-ksm

# If set, then we use the pre-saved block data to scan
USE_META=1
MONGO_DB_META_NAME=meta-ksm

# If set, then scan the known existed blocks first, the scan the next blocks one by one
USE_KNOWN_HEIGHTS=1
MONGO_DB_KNOWN_HEIGHTS_URL=mongodb://127.0.0.1:27017
MONGO_DB_KNOWN_HEIGHTS_NAME=known-heights-ksm

SCAN_STEP=100

CHAIN=kusama
